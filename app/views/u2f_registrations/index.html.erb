<h1>Your U2F Devices</h1>

<table>
  <thead>
    <tr>
      <th>Key Handle</th>
      <th>Public Key</th>
      <th>Certificate</th>
      <th>Counter</th>
      <th>Created At</th>
      <th>Updated At</th>
    </tr>
  </thead>

  <tbody>
    <% @u2f_devices.each do |device| %>
      <tr>
        <td><%= device.key_handle %></td>
        <td><%= device.public_key %></td>
        <td><%= device.certificate %></td>
        <td><%= device.counter %></td>
        <td><%= device.created_at %></td>
        <td><%= device.updated_at %></td>
        <td><%= link_to 'Destroy', user_u2f_registration_path(current_user, device), method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New U2F Device', new_user_u2f_registration_path(current_user) %>
